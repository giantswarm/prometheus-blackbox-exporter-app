                            "type": "object",
                            "properties": {
                                "dns": {
                                    "type": "object",
                                    "properties": {
                                        "preferred_ip_protocol": {
                                            "type": "string"
                                        },
                                        "query_name": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "prober": {
                                    "type": "string"
                                }
                            }
                        },
                        "dns_udp_internal": {
                            "type": "object",
                            "properties": {
                                "dns": {
                                    "type": "object",
                                    "properties": {
                                        "preferred_ip_protocol": {
                                            "type": "string"
                                        },
                                        "query_name": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "prober": {
                                    "type": "string"
                                }
                            }
