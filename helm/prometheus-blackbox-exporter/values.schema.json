                        "dns_tcp_internal": {
                            "type": "object",
                            "properties": {
                                "dns": {
                                    "type": "object",
                                    "properties": {
                                        "preferred_ip_protocol": {
                                            "type": "string"
                                        },
                                        "query_name": {
                                            "type": "string"
                                        },
                                        "transport_protocol": {
                                            "type": "string"
                                        }
                                    }
                                },
                                "prober": {
                                    "type": "string"
                                }
                            }
