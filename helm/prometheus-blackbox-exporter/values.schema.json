                                    "type": "object",
                                    "properties": {
                                        "follow_redirects": {
                                            "type": "boolean"
                                        },
                                        "preferred_ip_protocol": {
                                            "type": "string"
                                        },
                                        "valid_http_versions": {
                                            "type": "array",
                                            "items": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                },
                                "prober": {
                                    "type": "string"
                                },
                                "timeout": {
                                    "type": "string"
                                }
